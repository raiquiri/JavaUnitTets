<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Positive sort tests" verbose="1">
    <test name="Positive">
        <groups>
            <run>
                <include name="positive"/>
            </run>
        </groups>
        <classes>
            <class name="org.example.TestSortShaker"/>
            <class name="org.example.TestSortCombat"/>
            <class name="org.example.TestSortInsert"/>
            <class name="org.example.TestSortChoose"/>
        </classes>
    </test>
</suite>